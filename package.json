{
	"name": "javascript-development-environment",
	"version": "1.0.0",
	"description": "JavaScript development environment Pluralsight course by Cory House",
	"scripts": {
		"prestart": "node start.message.js",
		"start": "npm-run-all --parallel security-check tsc:w open:src",
		"open:src": "node server/server.js",
		"security-check": "nsp check",
		"localtunnel": "lt --port 8000",
		"share": "npm-run-all --parallel open:src localtunnel",
		"tsc": "tsc",
		"tsc:w": "tsc -- --watch"
	},
	"author": "Cory House",
	"license": "MIT",
	"dependencies": {
		"whatwg-fetch": "1.0.0"
	},
	"devDependencies": {
		"@types/body-parser": "^1.16.8",
		"@types/express": "^4.11.1",
		"@types/node": "^9.6.0",
		"@types/webpack": "^4.1.2",
		"chai": "3.5.0",
		"chalk": "1.1.3",
		"cheerio": "0.22.0",
		"compression": "1.6.2",
		"cross-env": "3.1.3",
		"css-loader": "0.25.0",
		"express": "4.14.0",
		"extract-text-webpack-plugin": "1.0.1",
		"html-webpack-plugin": "2.22.0",
		"jsdom": "9.8.0",
		"json-schema-faker": "0.3.6",
		"json-server": "0.8.22",
		"localtunnel": "1.8.1",
		"mocha": "3.1.2",
		"nock": "8.1.0",
		"npm-run-all": "3.1.1",
		"nsp": "2.6.2",
		"numeral": "1.5.3",
		"open": "0.0.5",
		"rimraf": "2.5.4",
		"style-loader": "0.13.1",
		"ts-loader": "^4.1.0",
		"tslint": "^5.9.1",
		"typescript": "^2.7.2",
		"webpack": "1.13.2",
		"webpack-dev-middleware": "1.8.4",
		"webpack-hot-middleware": "2.13.0",
		"webpack-md5-hash": "0.0.5"
	}
}
